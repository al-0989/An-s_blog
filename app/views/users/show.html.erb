<div class="main-page">
  <h1>User Profile</h1>
  <h5>First Name: <%= @user.first_name %></h5>
  <h5>Last Name: <%= @user.last_name %></h5>
  <h5>Email: <%= @user.email %></h5>
  <hr align="center" width="50%">
  <%= link_to "Edit Profile", edit_user_path(@user) %> |
  <%= link_to "Change Password", edit_password_path(@user)  %>
</div>
